{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "build",
      "request": "launch",
      "runtimeArgs": ["run", "build"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
       "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/Back"  // Asegúrate de que VS Code use la carpeta correcta
    },
      {
      "name": "start:dev",
      "request": "launch",
      "runtimeArgs": ["run", "start:dev"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/Back"  // Asegúrate de que VS Code use la carpeta correcta
    }
  ]
}